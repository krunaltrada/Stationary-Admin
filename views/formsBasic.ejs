<%- include('header') %>
    <div class="breadcrumbs">
        <div class="col-sm-4">
            <div class="page-header float-left">
                <div class="page-title">
                    <h1>Dashboard</h1>
                </div>
            </div>
        </div>
        <div class="col-sm-8">
            <div class="page-header float-right">
                <div class="page-title">
                    <ol class="breadcrumb text-right">
                        <li><a href="#">Dashboard</a></li>
                        <li><a href="#">Forms</a></li>
                        <li class="active">Basic</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>

    <div class="content">
        <div class="animated fadeIn">
            <div class="row">
                <div class="col-lg-12">
                    <div class="card mb-0">
                        <div class="card-header">
                            <strong>Stationary</strong> Form
                        </div>
                        <div class="card-body card-block">
                            <form action="" method="post" enctype="multipart/form-data" class="form-horizontal row">
                                <div class="col-6">
                                    <div class="row form-group">
                                        <div class="col col-md-4"><label for="text-input" class=" form-control-label">Product Name</label></div>
                                        <div class="col-12 col-md-8"><input type="text" id="text-input" name="pname" placeholder="Product Name" class="form-control"></div>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="row form-group">
                                        <div class="col col-md-4"><label for="text-input" class=" form-control-label">Company Name</label></div>
                                        <div class="col-12 col-md-8"><input type="text" id="text-input" name="cname" placeholder="Company Name" class="form-control"></div>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="row form-group">
                                        <div class="col col-md-4"><label for="text-input" class=" form-control-label">Quantity</label></div>
                                        <div class="col-12 col-md-8"><input type="text" id="text-input" name="quantity" placeholder="Quantity" class="form-control"></div>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="row form-group">
                                        <div class="col col-md-4"><label for="text-input" class=" form-control-label">Price</label></div>
                                        <div class="col-12 col-md-8"><input type="text" id="text-input" name="price" placeholder="Price" class="form-control"></div>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="card-footer d-flex justify-content-end">
                                        <button type="submit" class="btn btn-primary btn-sm mr-3">
                                            <i class="fa fa-dot-circle-o"></i> Submit
                                        </button>
                                        <button type="reset" class="btn btn-danger btn-sm">
                                            <i class="fa fa-ban"></i> Reset
                                        </button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- .animated -->
    </div>
    <%- include('footer') %>